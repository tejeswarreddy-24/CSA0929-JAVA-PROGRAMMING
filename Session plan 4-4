def multiplication_table(m, n):
    for i in range(1, n+1):
        print(m, "x", i, "=", m*i)

def main():
    m = int(input("Enter the number: "))
    n = int(input("Enter the range: "))
    print("Multiplication table of", m, "up to", n, ":")
    multiplication_table(m, n)

if _name_ == "_main_":
    main()
